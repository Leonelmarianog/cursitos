{
  "name": "cursitos-front",
  "version": "1.0.0",
  "description": "Frontend of cursitos.",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf ./build",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress",
    "postbuild": "npm run serve:build",
    "serve:build": "http-server -c-1 ./build",
    "dev": "cross-env NODE_ENV=development webpack serve --config webpack.config.js --progress",
    "lint:check": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx}",
    "format:check": "prettier --check src/**/*.{js,jsx,ts,tsx}",
    "format:fix": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "test": "jest",
    "test:dev": "jest --watchAll",
    "test:dev:coverage": "jest --coverage --watchAll"
  },
  "author": "Leonel Gauna",
  "license": "MIT",
  "dependencies": {
    "@chakra-ui/react": "^1.6.10",
    "@emotion/react": "^11.5.0",
